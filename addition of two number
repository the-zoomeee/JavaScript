var X,Y,Z;
X=parseInt(prompt("ENTER VALUE FOR X"));
Y=parseInt(prompt("ENTER VALUE FOR Y"));
Z=X-Y;
alert(Z);

var P,Q,R;
P=parseInt(prompt("ENTER VALUE FOR P"));
Q=parseInt(prompt("ENTER VALUE FOR Q"));
R=P*Q;
alert(R);

var L,M,N;
L=parseInt(prompt("ENTER VALUE FOR L"));
M=parseInt(prompt("ENTER VALUE FOR M"));
N=L/M;
alert(N);

